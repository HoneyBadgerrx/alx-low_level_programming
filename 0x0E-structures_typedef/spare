#include "dog.h"
#include <stddef.h>
/**
 * print_dog - prints types of dog
 * @d: name of structure dog
 * Return: nothing
 */
void print_dog(struct dog *d)
{
	if (d->name != NULL)
		printf("Name: %s\n", (*d).name);
	else
		printf("Name: (nil)\n");
	if ((*d).age != NULL)
		printf("Age: %f\n", (*d).age
